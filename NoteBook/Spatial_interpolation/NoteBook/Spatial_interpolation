        "%%R\n",
        "k<-1/lambda\n",
        "OK$OK.pred <-((OK$var1.pred *lambda +1)^k)\n",
        "OK$OK.var <-((OK$var1.var *lambda +1)^k)\n",
        "summary(OK)"
