    {
      "cell_type": "markdown",
      "source": [
        "### Make Prediction Grid"
      ],
      "metadata": {
        "id": "WDbN9ikgwHG1"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "%%R\n",
        "bbox(bd)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "A2zAzHOswGjV",
        "outputId": "21c5cd37-71f7-4bb3-908e-51c6354ae539"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "        min       max\n",
            "x  298521.7  778071.3\n",
            "y 2276992.7 2946935.1\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "%%R\n",
        "res <- 5000 # resolution\n",
        "# round extremes to resolution\n",
        "x.min <- bbox(bd)[1,1]%/%res*res\n",
        "x.max <- (bbox(bd)[1,2]+res)%/%res*res # make sure it is outside the bbox\n",
        "\n",
        "y.min <- bbox(bd)[2,1]%/%res*res\n",
        "y.max <- (bbox(bd)[2,2]+res)%/%res*res\n",
        "\n",
        "grid.df <- expand.grid(x = seq(x.min, x.max, by=res),\n",
        "              y = seq(y.min, y.max, by=res))\n",
        "head(grid.df)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "sb_LvgaqwVWH",
        "outputId": "e7cba62e-2e21-4331-d310-3a3b68ac0a15"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "       x       y\n",
            "1 295000 2275000\n",
            "2 300000 2275000\n",
            "3 305000 2275000\n",
            "4 310000 2275000\n",
            "5 315000 2275000\n",
            "6 320000 2275000\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "%%R\n",
        "##  define coordinates\n",
        "grid.xy <- grid.df[,c(1,2)]\n",
        "# Convert to spatial point\n",
        "grid.SPDF <- SpatialPointsDataFrame(coords = grid.xy, data=grid.df)\n",
        "# Define projection\n",
        "proj4string(grid.SPDF) = CRS(proj4string(bd) )\n"
