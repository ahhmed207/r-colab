        "%%R\n",
        "mf$As.bc<-bcPower(mf$As,lambda)\n",
        "head(mf)"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "### Create a SPDF"
      ],
      "metadata": {
        "id": "gIZ34_rZjPaW"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "%%R\n",
        "##  define coordinates\n",
        "xy <- mf[,c(2,3)]\n",
        "# Convert to spatial point\n",
        "SPDF <- SpatialPointsDataFrame(coords = xy, data=mf)\n",
        "# Define projection\n",
        "proj4string(SPDF) = CRS(\"+proj=longlat +ellps=WGS84\")  # WGS 84\n",
        "# Change projection\n",
        "BUTM<-proj4string(bd)              # extract projection information\n",
        "SPDF.PROJ<- spTransform(SPDF,      # Input SPDF\n",
        "                        BUTM)    # projection"
      ],
      "metadata": {
        "id": "sO1jG-r7u6eV"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "%%R\n",
        "# convert to a data-frame\n",
        "point.df<-as.data.frame(SPDF.PROJ)\n",
        "# Rename (last two column)\n",
        "colnames(point.df)[9] <- \"x\"\n",
        "colnames(point.df)[10] <- \"y\"\n",
        "head(point.df)"
      ],
      "metadata": {
